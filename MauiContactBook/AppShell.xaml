<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiContactBook.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiContactBook"
    xmlns:pages="clr-namespace:MauiContactBook.Pages"
    Title="MauiContactBook">

    <ShellContent Title="All Contacts" ContentTemplate="{DataTemplate pages:ContactListPage}" Route="ContactListPage" />
    <ShellContent Title="Add Contact" ContentTemplate="{DataTemplate pages:ContactAddPage}" Route="ContactAddPage" />
    <ShellContent Title="Edit Contact" ContentTemplate="{DataTemplate pages:ContactEditPage}" Route="ContactEditPage" />


</Shell>
